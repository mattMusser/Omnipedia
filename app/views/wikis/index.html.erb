<h1>All The Wikis</h1>
<hr>
<div class="row">
	<div class="col-md-8">
		<% @wikis.each do |wiki| %>
			<div class="media">
				<div class="media-body">
					<h4 class="media-heading">
						<p><%= link_to markdown(wiki.title), wiki_path(wiki.id) %></p>
					</h4>
				</div>
			</div>
		<% end %>
	</div class="col-md-4">
		<%= link_to "New Wiki", new_wiki_path, class: 'btn btn-success' %>
	</div>
</div>
